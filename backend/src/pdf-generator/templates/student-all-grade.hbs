<html lang='en'>

  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <title>Document</title>
    <style>
      /**body { font-family: 'Liberation Sans'; }*/ .container{ width: 100%;
      margin: 0 auto; } .header { position: fixed; top: 0px; left: 0px; right:
      0px; height: 100px; } .footer { position: fixed; bottom: 0px; left: 0px;
      right: 0px; height: 120px; } body { font-family: 'Arial' } table{ width:
      100%; } table,th, td { page-break-inside: auto; } .strong { font-weight:
      bold; } .bordered { border: 1px solid black; } .font-10 { font-size: 10px;
      } .font-12 { font-size: 12px; } .font-14 {font-size: 14px; } .font-16 {
      font-size: 16px; } .font-18 { font-size:18px; }.font-24 { font-size:24px;
      } .extra-spacing-tr { height: 20px; } .header { height: 60px; } tr
      {page-break-inside: avoid; page-break-after: auto; } thead {
      display:table-header-group; } tfoot { display: table-footer-group; }
      .to_title{ margin-top: 0px;}.to_title_div{ text-align: center; }
      .to_content{page-break-inside: auto; margin-top: 120px; margin-left: 40px;
      margin-right: 40px; } .to_number{margin-right: 100px;} .employee_table{
      }.recommending_approval{ display: flex; justify-content: space-between; }
      .underline{ border-bottom:1px solid black } #eval_table tr td, #eval_table
      tr th{ padding: 8px; } #eval_table{ page-break-after: always; } #f_table
      th,#f_table td,#eval_table th,#eval_table td{ border: 1px solid black; }
      #f_table,#eval_table, #mystudentGrade{ border-collapse: collapse; } 
      #eval_table{ border: 1px solid black; } #mystudentGrade  td{ border:
      1px solid black; padding: 2px;width:  11.11%; align-content: center;} 
    </style>
  </head>

  <body>
    {{#each this}}

      <div class='container'>
        <div class='header'>
          <img
            src='data:image/png;base64, {{header_img}}'
            width='100%'
            alt=''
          />

        </div>
        <div class='footer'>

          {{!-- <img
            style='position:fixed; bottom:0; left:0;right:0;'
            src='data:image/png;base64, {{footer_img}}'
            width='100%'
            alt=''
          /> --}}
        </div>

        
          <div style='width: 100%;'>
            <table
              style='margin:0 auto; width: 100%; page-break-inside:auto; table-layout:fixed'
            >
              {{! // Add this css to make main table fixed, child tables will still scroll }}
              <thead>
                <tr>
                  <td colspan='2'>
                    <div style='height: 110px;'></div>
                  </td>
                </tr>
              </thead>
              <tfoot>
                <tr>
                  <td colspan='2'>
                    <div style='height: 80px;'></div>

                  </td>
                </tr>
              </tfoot>
        
              <tbody class=""> 
                <tr>
                    <td colspan='' >
                        {{!-- <tr class="strong " >
                        <td class="font-14" style="padding-top:20px">School ID:&nbsp;<u>316401</u></td>
                        <td class="font-14" style="padding-top:20px" align="center">Region:&nbsp;<u>XI</u></td>
                        <td class="font-14" style="padding-top:20px" align="center">Division:&nbsp;<u></u></td>
                        <td class="font-14" style="padding-top:20px" align="center">District:&nbsp;<u></u></td>
                        </tr> --}}

                        <tr class="strong" >
                        {{!-- <td class="font-14" style="padding-top:20px">School Name:&nbsp;<u>Southern Davao National High School</u></td> --}}
                        <td class="font-14" style="padding-top:20px">School Year:&nbsp;<u>{{schoolYear}}</u></td>
                        <td class="font-14" style="padding-top:20px"  align="center">Grade Level:&nbsp;<u>{{gradeLevel}}</u></td>
                        {{#ifCond semester 'Junior High'}}
                            {{else}}
                        <td class="font-14" style="padding-top:20px" align="center">Semester:&nbsp;<u>{{semester}}</u></td>
                        {{/ifCond}}
                        </tr>

                        <tr class="strong" >
                    
                        <td class="font-14" style="padding-top:20px;">Teacher:&nbsp;<u style="text-transform: uppercase;">{{roomData.name}}</u></td>
                        <td class="font-14" style="padding-top:20px;" align="center">Section:&nbsp;<u style="text-transform: uppercase;">{{roomData.section_name}}</u></td>
                        </tr>
                    
                    </td>
                </tr>
               

                <tr>
            <td colspan='4'>
               <table class="mystudentGrade" border="1" style="table-layout: fixed; width: 100%; border-collapse: collapse; margin-top:20px">
              <thead>
                    <tr>
                        {{!-- <th rowspan="2" style="width: 2%;">No.</th> --}}
                        <th rowspan="2" style="width: 10%;">Name</th>

                        {{#each data.subjects}}
                        {{#ifCond ../semester 'Junior High'}}
                        <th colspan="5" >{{this}}</th>
                         {{else}}
                        <th colspan="3" >{{this}}</th>
                        {{/ifCond}}
                        {{/each}}
                     
                       

                        <th rowspan="2" style="width: 5%;">Final Average</th>
                        <th rowspan="2" style="width: 5%;">Status</th>
                    </tr>
                    <tr>
                        {{#each data.subjects}}
                        <th>1st Q</th>
                        <th>2nd Q</th>
                        {{#ifCond ../semester 'Junior High'}}
                        <th>3rd Q</th>
                        <th>4th Q</th>
                        {{else}}
                        {{/ifCond}}
                        <th>Final</th>
                        {{/each}}
                    </tr>
                </thead>
                <tbody>
                {{#each data.students}}
                    <tr>
                    {{!-- <td align="center">{{id}}</td> --}}
                    <td >{{name}}</td>

                    {{#each ../data.subjects}}
                        <td align="center">{{lookup (lookup ../this this) "q1"}}</td>
                        <td align="center">{{lookup (lookup ../this this) "q2"}}</td>
                        {{#ifCond ../../semester 'Junior High'}}
                        <td align="center">{{lookup (lookup ../this this) "q3"}}</td>
                        <td align="center">{{lookup (lookup ../this this) "q4"}}</td>
                          {{else}}
                        {{/ifCond}}
                        <td align="center">{{lookup (lookup ../this this) "final"}}</td>
                    {{/each}}

                    <td align="center">{{finalAverage}}</td>
                    <td align="center">
                        {{#ifCond status "Passed"}}
                        <span style="color: green;">Passed</span>
                        {{else}}
                        <span style="color: red;">Failed</span>
                        {{/ifCond}}
                    </td>
                    </tr>
                {{/each}}
                </tbody>

                </table>
                </tr>
                <tr class="font-20"> 
                    <td>
                      <div style="margin-top: 35px;">
                        Prepared by: <u>{{roomData.name}}</u>
                      </div>
                    </td>
                </tr>
             
              </tbody>
            </table>
          </div>
      </div>
    {{/each}}
  </body>

</html>